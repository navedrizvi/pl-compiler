/* Test: relational operators */

program basic_relational_ops
let
begin

    function main() : int
    begin
        let
            var a : int := 1;
            var b : int := 2;
            var c : int := 3;
        begin
            printi(a == 1);
            printi(a != 1);
            printi(a > b);
            printi(a < b);
            printi(a <= c);
            printi(a <= 1);
            printi(a >= c);
            printi(a >= 1);
        end
        return 0;
    end

end
