/* Test: power operator */

program basic_math_power_op
let
begin

    function main() : int
    begin
        let
            var a : int := 2;
            var b : int;
        begin
            b := 10 ** 2;
            printi(b);

            b := 3 ** a ** 3;
            printi(b);

            b := 9 / 3 ** a * 12;
            printi(b);
        end
        return 0;
    end

end
