/* Test: combination of logical and relational operators */

program basic_logical_relational_combo
let
begin

    function main() : int
    begin
        let
            var a : int := 1;
            var b : int := 2;
            var c : int := 3;
        begin
            printi((a<=b) <= c);
            printi(a != b | c >= a);
            printi((a<b) & c);
            printi(a < b & c > b);
            printi(a > b | a < c);
            printi(a > b & b > c | c < 4);
        end
        return 0;
    end

end
