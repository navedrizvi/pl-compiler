/* Test: nested while loop */

program loop_nested_while
let
begin

    function main() : int
    begin
        let
            var i, j : int;
        begin
            i := 0;
            while (i < 3) do

                j := 0;
                while (j <= 3) do
                    printi(i + j);
                    j := j + 1;
                enddo;

                i := i + 1;
            enddo;
        end
        return 0;
    end

end
